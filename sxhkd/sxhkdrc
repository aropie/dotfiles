# BASIC
super + Return
        $TERMINAL
super + o
	rofi -show drun -show-icons
super + shift + Escape
        rofi_power
super + r
        $TERMINAL -e ranger
super + shift + x
        xautolock -locknow
super + shift + a
        rofi_audioswap
super + p
        rofi_password
super + shift + p
        rofi_generate_password
super + e
	emacsclient -cn -a ""

# MEDIA
XF86AudioLowerVolume
        pamixer --allow-boost -d 5
super + less
        pamixer --allow-boost -d 5
XF86AudioRaiseVolume
        pamixer --allow-boost -i 5
super + shift + less
        pamixer --allow-boost -i 5
XF86AudioMute
        pamixer -t
XF86AudioPlay
	mpc -q toggle
XF86Audio{Prev,Next}
	mpc -q {cdprev,next}
XF86MonBrightnessDown
	xbacklight -dec 20
XF86MonBrightnessUp
	xbacklight -inc 20
Print
	rofi_screenshot
